ssh -i "david-ai-callbot-server.pem" ubuntu@ec2-52-70-201-226.compute-1.amazonaws.com

sudo cp -r /home/ubuntu/fiverr-david-call-bot-frontend/dist/* /var/www/frontend/


sudo systemctl restart nginx

sudo nano /etc/nginx/sites-available/frontend
sudo nano /etc/nginx/sites-available/backend

sudo systemctl restart nginx


